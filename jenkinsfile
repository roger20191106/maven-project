pipeline {
    agent any
    tools{
        maven 'local maven'
    }
    stages{
        stage ('Build'){
            steps{
                echo 'Start building ...'
                mvn clean package
                echo 'Builded!'
            }
			post{
				success{
					echo 'Star to archive...'
					archiveArtifacts artifacts: '**/target/*.war'
				}
			}
        }
    } 
}