pipeline {
    agent any
    tools{
        maven 'local maven'
    }
    stages{
        stage ('Build'){
            steps{
                echo 'Stat building ...'
                sh 'mvn clean package'
            }
			post{
				sucess{
					echo 'Star to archive...'
					archiveArtifacts artifacts: '**/target/*.war'
				}
			}
        }
    } 
}