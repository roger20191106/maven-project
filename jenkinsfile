pipeline {
    agent any
    tools{
        maven 'local maven'
    }
    stages{
        stage ('Build'){
            steps{
                echo 'Start building ...'
                cmd.exe /C "D:\Tools\apache-maven-3.9.6\bin\mvn.cmd clean package && exit %%ERRORLEVEL%%"
                echo 'Builded!'
            }
			post{
				success{
					echo 'Star to archive...'
					archiveArtifacts artifacts: '**/target/*.war'
				}
			}
        }
    } 
}