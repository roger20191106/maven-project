pipeline {
    agent any
    tools{
        maven 'local maven'
    }
    stages{
        stage('Init'){
            steps{
                echo "Init ...."
            }
        }
        stage('Build'){
            steps{
                echo 'Start building ...'
                bat 'mvn clean package'
                echo 'Builded!'
            }
         	post{
				sucess{
					echo "Star to archive..."
					archiveArtifacts artifacts: "**/target/*.war"
				}
			}
		}
        stage('Deploy'){
            steps{
                echo 'Start Deploying ...'
            }
        }
    } 
}