pipeline {
    agent any
    tools{
        maven 'local maven'
    }
    stages{
        stage('Init'){
            steps{
                echo "Init ...."
            }
        }
        stage ('Build'){
            steps{
                echo 'Start building ...'
                bat 'mvn clean package'
                echo 'Builded!'
            }
		}
        stage('Deploy'){
            steps{
                echo 'Start Deploying ...'
            }
        }
    } 
}